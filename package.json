{
  "name": "fortepan",
  "version": "1.0.0",
  "description": "Fortepan.hu public dev repository",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rm -rf _dist",
    "eleventy:default": "ELEVENTY_ENV=development npx eleventy ",
    "eleventy:watch": "ELEVENTY_ENV=development npx eleventy --watch",
    "eleventy:prod": "ELEVENTY_ENV=production npx eleventy",
    "scripts:prod": "rollup --config rollup.config.js --environment ENV:production",
    "scripts:dev": "rollup --config rollup.config.js --environment ENV:development",
    "styles:prod": "POSTCSS_ENV=production postcss src/styles.scss -o _dist/app.css",
    "styles:dev": "POSTCSS_ENV=development postcss src/styles.scss -o _dist/app.css",
    "light-server:serve": "light-server -c .lightserverrc",
    "dev": "npm-run-all -l --silent clean styles:dev scripts:dev eleventy:default --parallel eleventy:watch light-server:serve",
    "dev:debug": "DEBUG=* npm run dev",
    "build": "run-s clean styles:prod scripts:prod eleventy:prod"
  },
  "repository": {
    "type": "git",
    "url": "git+https://richardgazdik@github.com/richardgazdik/fortepan.hu.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/richardgazdik/fortepan.hu/issues"
  },
  "homepage": "https://github.com/richardgazdik/fortepan.hu#readme",
  "devDependencies": {
    "@11ty/eleventy": "^0.9.0",
    "@babel/core": "^7.8.7",
    "@babel/preset-env": "^7.8.7",
    "@csstools/postcss-sass": "^4.0.0",
    "acorn": "^7.1.1",
    "add": "^2.0.6",
    "autoprefixer": "^9.7.4",
    "babel-loader": "^8.0.6",
    "core-js": "^3.6.4",
    "cssnano": "^4.1.10",
    "cssnano-preset-advanced": "^4.0.7",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-prettier": "^3.1.2",
    "glob": "^7.1.6",
    "light-server": "2.6.4",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^6.1.3",
    "postcss-devtools": "^1.1.1",
    "postcss-hash": "^1.0.2",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-scss": "^2.0.0",
    "prettier": "^1.19.1",
    "regenerator-runtime": "^0.13.3",
    "rollup": "^1.32.1",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-hash": "^1.3.0",
    "rollup-plugin-node-globals": "^1.4.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-output-manifest": "^1.0.2",
    "rollup-plugin-terser": "^5.3.0",
    "stylelint": "^12.0.1",
    "stylelint-prettier": "^1.1.2",
    "terser": "^4.6.6"
  },
  "dependencies": {
    "custom-event-polyfill": "^1.0.7",
    "lodash": "^4.17.15"
  }
}
